{
    "name": "zalsader/MyMigrations",
    "description": "MyMigrations plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "cakephp/migrations": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "MyMigrations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyMigrations\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
